<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 设置填充颜色 为白色 -->
    <solid android:color="#ffffff" />
    <!-- 设置几何图形四个角的弧度 -->
    <corners
        android:radius="20dp"/>
    <!-- 设置几何形状绘制边框 红色边框 -->
    <stroke android:color="#fd7e18"
        android:width="1dp"/>
</shape>