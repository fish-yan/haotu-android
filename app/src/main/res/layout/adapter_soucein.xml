<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:gravity="center"
        android:layout_gravity="center"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_30"
        >
    <TextView

        android:layout_marginRight="30dp"
        android:layout_weight="1"
        android:layout_marginTop="10dp"
        android:id="@+id/tv_numbone"
        android:gravity="center"
        android:layout_gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="01"
        android:textColor="@color/black"
        android:textSize="14dp"
        />
        <TextView
            android:layout_marginLeft="30dp"
            android:layout_weight="1"
            android:layout_marginTop="10dp"
            android:id="@+id/tv_numb"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="01"
            android:textColor="@color/black"
            android:textSize="14dp"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal"
        >
<LinearLayout
    android:layout_weight="1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
    <LinearLayout

        android:gravity="center_vertical"
        android:layout_gravity="center_vertical"
        android:id="@+id/ll_orderset"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_80"
        android:orientation="horizontal"
        >

        <RelativeLayout
            android:layout_marginLeft="10dp"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
            <com.dmeyc.dmestoreyfm.wedgit.CircleImageView
                android:layout_centerInParent="true"
                android:id="@+id/iv_set1"
                android:layout_centerVertical="true"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@drawable/set_rabbit"
                />

            <LinearLayout
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/iv_set1"
                android:orientation="horizontal"
                >

                <TextView
                    android:id="@+id/tv_set1name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/iv_set1"
                    android:layout_centerInParent="true"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:text="ss"
                    android:textSize="@dimen/dimen_10" />
                <ImageView
                    android:visibility="gone"
                    android:id="@+id/iv_setsex1"
                    android:layout_marginLeft="2dp"
                    android:layout_width="@dimen/dimen_10"
                    android:layout_height="10dp"
                    android:background="@drawable/set_woman"
                    />
            </LinearLayout>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginRight="@dimen/dimen_10"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >
            <com.dmeyc.dmestoreyfm.wedgit.CircleImageView
                android:layout_centerInParent="true"
                android:id="@+id/iv_set2"
                android:layout_centerVertical="true"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@drawable/set_rabbit"
                />

            <LinearLayout
                android:id="@+id/ll_name"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/iv_set2"
                android:orientation="horizontal"
                >
                <TextView
                    android:layout_below="@+id/iv_set2"
                    android:id="@+id/tv_set2name"
                    android:layout_centerInParent="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ss"
                    android:textSize="@dimen/dimen_10"
                    />
                <ImageView
                    android:paddingRight="5dp"
                    android:visibility="gone"
                    android:id="@+id/iv_setsex2"
                    android:layout_marginLeft="2dp"
                    android:layout_width="@dimen/dimen_10"
                    android:layout_height="10dp"
                    android:background="@drawable/set_woman"
                    />
            </LinearLayout>

        </RelativeLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_gravity="center"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:background="@drawable/shap_rech"
        android:gravity="center"
        android:orientation="horizontal">
        <EditText
            android:focusable="false"
            android:clickable="false"
            android:id="@+id/et_numberone"
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:background="@null"
            android:gravity="center"
            android:inputType="number"
            android:maxLength="2"
            android:text="1" />
    </LinearLayout>
    </LinearLayout>


        <LinearLayout
            android:gravity="bottom"
            android:layout_gravity="bottom"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >
            <TextView
                android:gravity="center"
                android:layout_gravity="center"
                android:textColor="@color/c9"
                android:textSize="18dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="VS"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="32423432"
                />
        </LinearLayout>

        <LinearLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >

        <LinearLayout
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:layout_gravity="center_vertical"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_80"
            android:orientation="horizontal"
            >

            <RelativeLayout
                android:layout_marginLeft="10dp"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >
                <com.dmeyc.dmestoreyfm.wedgit.CircleImageView
                    android:layout_centerInParent="true"
                    android:layout_centerVertical="true"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:id="@+id/civ_rabit"
                    android:background="@drawable/set_rabbit"
                    />

                <LinearLayout
                    android:layout_below="@+id/civ_rabit"
                    android:gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/iv_set1"
                        android:layout_centerInParent="true"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="ss"
                        android:textSize="@dimen/dimen_10" />
                    <ImageView
                        android:visibility="gone"
                        android:layout_marginLeft="2dp"
                        android:layout_width="@dimen/dimen_10"
                        android:layout_height="10dp"
                        android:background="@drawable/set_woman"
                        />
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_marginRight="@dimen/dimen_10"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >
                <com.dmeyc.dmestoreyfm.wedgit.CircleImageView
                    android:layout_centerInParent="true"
                    android:layout_centerVertical="true"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:id="@+id/civ_rabit2"
                    android:background="@drawable/set_rabbit"
                    />

                <LinearLayout
                    android:layout_below="@+id/civ_rabit2"
                    android:gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    >
                    <TextView
                        android:layout_below="@+id/iv_set2"
                        android:layout_centerInParent="true"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="ss"
                        android:textSize="@dimen/dimen_10"
                        />
                    <ImageView
                        android:paddingRight="5dp"
                        android:visibility="gone"
                        android:layout_marginLeft="2dp"
                        android:layout_width="@dimen/dimen_10"
                        android:layout_height="10dp"
                        android:background="@drawable/set_woman"
                        />
                </LinearLayout>
            </RelativeLayout>
        </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:background="@drawable/shap_rech"
                android:gravity="center"
                android:orientation="horizontal">
                <EditText
                    android:focusable="false"
                    android:clickable="false"
                    android:id="@+id/et_numbertwo"
                    android:layout_width="40dp"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:gravity="center"
                    android:inputType="number"
                    android:maxLength="2"
                    android:text="1" />
            </LinearLayout>

        </LinearLayout>


    </LinearLayout>
</LinearLayout>