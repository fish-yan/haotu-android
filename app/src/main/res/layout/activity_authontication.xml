<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/layout_title" />

    <com.dmeyc.dmestoreyfm.wedgit.CustomItemView
        android:id="@+id/vic_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        app:civ_left_hint_str="请输入真实姓名"
        app:civ_type="5" />

    <com.dmeyc.dmestoreyfm.wedgit.CustomItemView
        android:id="@+id/vic_idcard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:civ_left_hint_str="请填写身份证号"
        app:civ_type="5" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            style="@style/text_14_9D9D9D"
            android:text="提交即表示同意" />

        <TextView
            android:id="@+id/tv_protocol"
            style="@style/text_14_9D9D9D"
            android:text="用户协议"
            android:textColor="@color/color_9A6500" />

        <TextView
            style="@style/text_14_9D9D9D"
            android:text="及"
            android:textColor="@color/color_9A6500" />

        <TextView
            android:id="@+id/tv_private"
            style="@style/text_14_9D9D9D"
            android:text="隐私条款"
            android:textColor="@color/color_9A6500" />

    </LinearLayout>
</LinearLayout>