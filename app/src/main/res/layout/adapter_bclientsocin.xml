<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"

    android:layout_marginTop="@dimen/dimen_10"
    android:layout_marginLeft="@dimen/dimen_10"
    android:layout_marginRight="@dimen/dimen_10"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_5"
        android:background="@color/gb"
        />
    <LinearLayout
        android:background="@drawable/shap_white_littleconner"
        android:layout_width="match_parent"
        android:layout_height="84dp"
        android:orientation="vertical"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:layout_weight="1"
                >

            <LinearLayout
                android:layout_marginRight="@dimen/dimen_5"
                android:layout_marginLeft="@dimen/dimen_10"
                android:layout_gravity="center_vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical"
                >

                <com.dmeyc.dmestoreyfm.wedgit.CircleImageView
                    android:id="@+id/cv_actionone"
                    android:layout_width="@dimen/dimen_35"
                    android:layout_height="@dimen/dimen_35"
                    android:layout_gravity="center"
                    android:background="@drawable/me_ic_head" />
                <TextView
                    android:singleLine="true"
                    android:maxLength="4"
                    android:ellipsize="end"
                    android:id="@+id/action_onename"
                    android:layout_marginTop="@dimen/dimen_5"
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="好图"
                    android:textSize="@dimen/dimen_12"
                    />
            </LinearLayout>

            <LinearLayout
                android:layout_marginRight="@dimen/dimen_5"
                android:layout_marginLeft="@dimen/dimen_10"
                android:layout_gravity="center_vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical"
                >
                <com.dmeyc.dmestoreyfm.wedgit.CircleImageView
                    android:id="@+id/cv_actiontwo"
                    android:layout_width="@dimen/dimen_35"
                    android:layout_height="@dimen/dimen_35"
                    android:background="@drawable/me_ic_head"
                    android:layout_gravity="center"
                    />
                <TextView
                    android:singleLine="true"
                    android:maxLength="4"
                    android:ellipsize="end"
                    android:id="@+id/action_twoname"
                    android:layout_marginTop="@dimen/dimen_5"
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="好图"
                    android:textSize="@dimen/dimen_12"
                    />
            </LinearLayout>


        </LinearLayout>

<LinearLayout
    android:id="@+id/ll_sockclick"
    android:paddingLeft="@dimen/dimen_10"
    android:paddingRight="@dimen/dimen_10"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_marginBottom="@dimen/dimen_15"
    >

    <LinearLayout
        android:gravity="center"
        android:layout_gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal"
        >
        <TextView
            android:id="@+id/tv_teamsocke"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="@dimen/dimen_30"
            android:layout_height="@dimen/dimen_50"
            android:text="0"
            android:textStyle="bold"
            android:textSize="@dimen/dimen_24"
            android:textColor="#8A8A8A"
            android:background="@null"
            />
        <TextView
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=":"
            android:textStyle="bold"
            />
        <TextView
            android:id="@+id/tv_teambsocker"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_width="@dimen/dimen_30"
            android:layout_height="@dimen/dimen_50"
            android:text="0"
            android:textSize="@dimen/dimen_24"
            android:textColor="#8A8A8A"
            android:background="@null"
            android:textStyle="bold"
            />
    </LinearLayout>
    <TextView
        android:textSize="@dimen/dimen_10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="男女单打第二轮"
        />
 </LinearLayout>
            <LinearLayout
