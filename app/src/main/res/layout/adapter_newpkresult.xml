<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_marginTop="@dimen/dimen_10"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_5"
        android:background="@color/gb"
        />
    <LinearLayout
        android:gravity="center_vertical"
        android:layout_gravity="center_vertical"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="84dp"
        android:orientation="vertical"
        >
        <!--android:background="@drawable/shap_timewhite"-->
        <LinearLayout
            android:gravity="center_vertical"
            android:layout_gravity="center_vertical"
            android:layout_below="@+id/ll_top"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <LinearLayout

                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                >
                <com.dmeyc.dmestoreyfm.wedgit.CircleImageView
                    android:id="@+id/iv_teamone1"
                    android:layout_gravity="center"
                    android:layout_width="@dimen/dimen_35"
                    android:layout_height="@dimen/dimen_35"
                    android:background="@drawable/me_ic_head"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_20"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:orientation="horizontal">
                <TextView
                    android:id="@+id/tv_teamonename1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"

                    android:ellipsize="end"
                    android:gravity="center"
                    android:maxWidth="@dimen/dimen_40"
                    android:singleLine="true"
                    android:text="中羽联"
                    android:textSize="@dimen/dimen_12" />
                    <ImageView
                        android:id="@+id/iv_sex1"
                        android:layout_width="@dimen/dimen_10"
                        android:layout_height="@dimen/dimen_10"
                        android:layout_gravity="center"
                        android:background="@drawable/woman_check" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                >
                <com.dmeyc.dmestoreyfm.wedgit.CircleImageView
                    android:id="@+id/iv_teamone2"
                    android:layout_gravity="center"
                    android:layout_width="@dimen/dimen_35"
                    android:layout_height="@dimen/dimen_35"
                    android:background="@drawable/me_ic_head"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_20"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:orientation="horizontal">
                <TextView
                    android:gravity="center"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:maxWidth="@dimen/dimen_40"
                    android:id="@+id/tv_teamonename2"
                    android:textSize="@dimen/dimen_12"

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="中羽联"
                    />
                    <ImageView
                        android:id="@+id/iv_sex2"
                        android:layout_width="@dimen/dimen_10"
                        android:layout_height="@dimen/dimen_10"
                        android:layout_gravity="center"
                        android:background="@drawable/woman_check" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                >

                <TextView
                    android:id="@+id/tv_socker"
                    android:layout_weight="2"
                    android:textSize="@dimen/dimen_24"
                    android:layout_marginTop="5dp"
                    android:textColor="@color/black"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="2 ：0"
                    />
                <TextView
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:maxWidth="@dimen/dimen_100"
                    android:layout_weight="1"
                    android:id="@+id/tv_date"
                    android:textSize="@dimen/dimen_10"
                    android:layout_marginTop="5dp"
                    android:textColor="@color/black"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="5月29日"
                    />
            </LinearLayout>


            <LinearLayout
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                >
                <com.dmeyc.dmestoreyfm.wedgit.CircleImageView
                    android:id="@+id/iv_teamtwo1"
                    android:layout_gravity="center"
                    android:layout_width="@dimen/dimen_35"
                    android:layout_height="@dimen/dimen_35"
                    android:background="@drawable/me_ic_head"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_20"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:orientation="horizontal">
                <TextView
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:maxWidth="@dimen/dimen_40"
                    android:id="@+id/tv_teantwonaem1"
                    android:textSize="@dimen/dimen_12"
                    android:gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="中羽联"
                    />
                    <ImageView
                        android:id="@+id/iv_sex3"
                        android:layout_width="@dimen/dimen_10"
                        android:layout_height="@dimen/dimen_10"
                        android:layout_gravity="center"
                        android:background="@drawable/woman_check" />
                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:layout_marginLeft="10dp"
                android:orientation="vertical"
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                >
                <com.dmeyc.dmestoreyfm.wedgit.CircleImageView
                    android:id="@+id/iv_teamtwo2"
                    android:layout_gravity="center"
                    android:layout_width="@dimen/dimen_35"
                    android:layout_height="@dimen/dimen_35"
                    android:background="@drawable/me_ic_head"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_20"
                    android:layout_gravity="center"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:orientation="horizontal">
                <TextView
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:maxWidth="@dimen/dimen_40"
                    android:gravity="center"
                    android:id="@+id/tv_teantwonaem2"
                    android:textSize="@dimen/dimen_12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="好图"
                    />
                    <ImageView
                        android:id="@+id/iv_sex4"
                        android:layout_width="@dimen/dimen_10"
                        android:layout_height="@dimen/dimen_10"
                        android:layout_gravity="center"
                        android:background="@drawable/woman_check" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>